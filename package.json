{
  "name": "nuxt-starter-core",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "nuxt": "^4.2.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/test-utils": "^3.22.0",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/nuxt": "^14.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nuxt-security": "^2.5.0",
    "typescript": "^5.9.3",
    "unplugin-turbo-console": "^2.3.0"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "darwin",
        "linux",
        "win32"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,md,json,yaml,yml,css,html}": [
      "eslint --fix"
    ]
  }
}
